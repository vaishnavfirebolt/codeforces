package day8;
import java.util.*;

public class CD432A {

	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	
	int n = sc.nextInt();
	int k = sc.nextInt();
	int a[] = new int[n];
	for(int i = 0 ; i < n ;i++) {
		a[i] = sc.nextInt();
	}Arrays.sort(a);
	int count = 0;
	for(int i = 2; i < a.length ;  i += 3) {
		if((5 - a[i] )>= k && (5 - a[i-1]) >=  k && (5 - a[i - 2]) >=  k) {
			count++;
		}
	}
			System.out.println(count);
	
	sc.close();

	}

}
