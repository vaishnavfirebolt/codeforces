package day66;

import java.util.Scanner;

public class Codeforces805B {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt(); 
		int count = 0 ;
		boolean isA = false;
		 for(int i = 0; i < n; i++) {
	            if(count == 2) {
	                count = 0;
	                isA = !isA;
	            }
	            if(isA) System.out.print('a');
	            else System.out.print('b');
	            count++;
	        }
		
		
		sc.close();
	}

}
