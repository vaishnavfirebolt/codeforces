package day74;

import java.util.Scanner;

public class Codeforces1325A {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for (int tt = 0; tt < t; tt++) {
            int x = sc.nextInt();
            System.out.println(1 + " " + (x - 1));
        }
		sc.close();
	}
}
