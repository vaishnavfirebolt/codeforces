package day15;
import java.util.*;

public class CD282B {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		StringBuilder sb = new StringBuilder();
		int n = sc.nextInt();
		long sum = 0 ;
		for(int i = 0; i < n ;i++) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			
		if(sum + a <= 500) {
			sum+= a;
			sb.append('A');
		}else {
			sum -= b;
			sb.append('G');
		}
			
		}
		System.out.println(sb);
		
		sc.close();
	}

}
