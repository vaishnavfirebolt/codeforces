package day58;

import java.util.*;

public class Codeforces1167A {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int t = sc.nextInt() ; 
		for(int i = 0 ; i < t ; i++) {
			int n = sc.nextInt();
			String s = sc.next();
			int k = s.indexOf('8');
			if(n - k >= 11 && k >= 0 ) {
				System.out.println("YES");
			}else System.out.println("NO");
		}
		sc.close();
	}
	
}
