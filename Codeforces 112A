import java.util.Scanner;
 
public class A {
    
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        
        String a = scan.nextLine().toLowerCase();
        String b = scan.nextLine().toLowerCase();
        
        int x = a.compareTo(b);
        
        if(x < 0) x = -1;
        else if(x > 0) x = 1;
        
        System.out.println(x);
    
    }
}
